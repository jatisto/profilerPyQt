/* Style for the entire QTableWidget */
QTableWidget {
    font-family: "JetBrains Mono", serif;
    font-size: 16px;
    width: 100%;
    border: none;
    margin-bottom: 20px;
}

/* Style for the header section of QTableWidget */
QTableWidget::header {
    font-weight: bold;
    text-align: left;
    border: none;
    padding: 10px 15px;
    background: #d8d8d8;
    font-size: 14px;
}

/* Border radius for the first header cell */
QTableWidget::header:first {
    border-radius: 8px 0 0 8px;
}

/* Border radius for the last header cell */
QTableWidget::header:last {
    border-radius: 0 8px 8px 0;
}

/* Style for the table data cells */
QTableWidget::item {
    text-align: left;
    /*border: 1px solid #d3d3d3;*/
    /*padding: 10px 15px;*/
    font-size: 25px;
    vertical-align: top;
}

/* Style for the even rows */
QTableWidget::item:alternate {
    background: #f3f3f3;
}

/* Border radius for the first data cell in each row */
QTableWidget::item:first {
    border-radius: 8px 0 0 8px;
}

/* Border radius for the last data cell in each row */
QTableWidget::item:last {
    border-radius: 0 8px 8px 0;
}

