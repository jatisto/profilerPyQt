/* Custom Grey Theme with Rounded Buttons and Widgets */

/* Default color for widgets */
QWidget
{
    background-color: rgb(220, 220, 220);
    color: rgb(60, 60, 60);
}

/* Default text color for disabled widgets */
QWidget:disabled
{
    color: rgb(150, 150, 150);
}

/* Text Input Widgets */
QLineEdit, QTextEdit, QPlainTextEdit
{
    background-color: rgb(240, 240, 240);
    color: rgb(60, 60, 60);
    selection-color: rgb(60, 60, 60);
    selection-background-color: rgba(100, 100, 100, 0.5);
    border: 1px solid rgb(200, 200, 200);
    border-radius: 5px;
    width: 200px;
    font-family: "JetBrains Mono", serif;
    font-size: 10pt;
}

QLineEdit:focus, QLineEdit:hover, QTextEdit:hover, QPlainTextEdit:hover
{
    border-color: rgb(40, 44, 52);
}

QTextEdit:focus, QPlainTextEdit:focus
{
    border: 1px solid rgb(100, 100, 100);
}

QLineEdit:disabled, QPlainTextEdit:disabled, QTextEdit:disabled
{
    background-color: rgb(230, 230, 230);
    color: rgb(150, 150, 150);
    border-color: rgb(200, 200, 200);
}

/* Buttons */
QPushButton
{
    color: rgb(60, 60, 60);
    padding: 8px;
    border-radius: 10px;
    background-color: rgb(200, 200, 200);
}

/* Button which is not flat */
QPushButton:!flat
{
    border: 1px solid rgb(150, 150, 150);
}

QPushButton:hover
{
    background-color: rgb(150, 150, 150);
}

QPushButton:pressed
{
    margin-top: 2px;
    margin-left: 2px;
    margin-bottom: -2px;
    margin-right: -2px;
}

/* Size of ok button in dialog box */
QDialogButtonBox [text="OK"]
{
    min-width: 3em;
}

QToolButton
{
    background-color: transparent;
}

QToolButton:hover
{
    background-color: rgba(150, 150, 150, 50%);
    border: 1px solid rgb(150, 150, 150);
    border-radius: 5px;
}

QPushButton:disabled:!flat
{
    background-color: rgb(230, 230, 230);
    border-color: rgb(180, 180, 180);
    color: rgb(150, 150, 150);
}

QPushButton:disabled:flat
{
    color: rgb(150, 150, 150);
}

/* Combo Box */
QComboBox
{
    background-color: rgb(240, 240, 240);
    selection-color: rgb(60, 60, 60);
    selection-background-color: rgba(100, 100, 100, 50%);
    border: 1px solid rgb(200, 200, 200);
    padding: 3px 18px 3px 5px;
    min-width: 10em;
    border-radius: 5px;
}

QComboBox:on
{
    padding-top: 5px;
    padding-left: 6px;
}

QComboBox:hover, QComboBox:focus
{
    border-color: rgb(100, 100, 100);
}

QComboBox:disabled
{
    background-color: rgb(230, 230, 230);
    color: rgb(150, 150, 150);
    border-color: rgb(200, 200, 200);
}

QComboBox::drop-down
{
    width: 15px;
    background-color: rgb(200, 200, 200);
}

QComboBox::drop-down:hover, QComboBox::drop-down:focus
{
    border-color: rgb(100, 100, 100);
    background-color: rgb(150, 150, 150);
}

QComboBox::drop-down:disabled
{
    background-color: rgb(230, 230, 230);
}

QComboBox::down-arrow
{
    image: url(:/icons/grey/combo_box_down_arrow.png);
    width: 15px;
    height: 100%;
}

QComboBox::down-arrow:on
{
    top: 1px;
    left: 1px;
}

QComboBox::down-arrow:disabled
{
    image: url(:/icons/grey/combo_box_down_arrow_disabled.png);
}

QComboBox QAbstractItemView
{
    border: 1px solid rgb(100, 100, 100);
}

/* Menu bar Tool bar Menu and Menu Items */

QMenuBar
{
    background-color: rgb(220, 220, 220);
}

QMenuBar::item:selected
{
    background-color: rgb(150, 150, 150);
}

QMenuBar:disabled
{
    background-color: rgb(230, 230, 230);
}

QToolBar
{
    background-color: rgb(200, 200, 200);
    border: 1px solid rgb(150, 150, 150);
}

QToolBar QToolButton
{
    background-color: transparent;
}

QToolBar QToolButton:hover
{
    background-color: rgba(150, 150, 150, 50%);
    border: 1px solid rgb(150, 150, 150);
    border-radius: 5px;
}

QToolBar QToolButton:checked
{
    background-color: rgba(150, 150, 150, 50%);
    border: 1px solid rgb(150, 150, 150);
    border-radius: 5px;
}

QToolBar QToolButton:disabled
{
    color: rgb(150, 150, 150);
}

QMenu
{
    background-color: rgb(240, 240, 240);
    border: 1px solid rgb(200, 200, 200);
    /* Extra stretch */
    margin: 2px;
    border-radius: 5px;
}

QMenu::item
{
    background-color: transparent;
    padding: 4px 22px 4px 22px;
    border-radius: 5px;
}

QMenu::item:selected
{
    color: rgb(60, 60, 60);
}

QMenu::item:disabled
{
    color: rgb(150, 150, 150);
}

QMenu::indicator
{
    width: 13px;
    height: 13px;
    /* When the menu is non-exclusive, the indicator looks disabled */
    image: url(:/icons/grey/menu-indicator.png);
}

QMenu::indicator:checked
{
    image: url(:/icons/grey/menu-indicator-checked.png);
}

QMenu::right-arrow
{
    margin: 5px;
    width: 5px;
    height: 11px;
    image: url(:/icons/grey/menu-right-arrow.png);
}

QMenu::right-arrow:disabled
{
    image: url(:/icons/grey/menu-right-arrow-disabled.png);
}

/* Scroll Bar */

QScrollBar:vertical
{
    border: 1px solid rgb(200, 200, 200);
    background: rgb(240, 240, 240);
    width: 15px;
    margin: 22px 0 22px 0;
    border-radius: 4px;
}

QScrollBar::handle:vertical
{
    background: rgb(200, 200, 200);
    min-height: 20px;
    border-radius: 4px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical,
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical
{
    height: 0px;
}

/* Sliders */

QSlider::groove:horizontal
{
    border: 1px solid rgb(200, 200, 200);
    background: rgb(220, 220, 220);
    height: 8px;
    border-radius: 4px;
}

QSlider::handle:horizontal
{
    background: rgb(200, 200, 200);
    width: 14px;
    margin-top: -3px;
    margin-bottom: -3px;
    border-radius: 7px;
}

QSlider::add-page:qlineargradient
{
    background: rgb(220, 220, 220);
    border: 1px solid rgb(200, 200, 200);
    height: 8px;
    border-radius: 4px;
}

QSlider::sub-page:qlineargradient
{
    background: rgb(200, 200, 200);
    border: 1px solid rgb(200, 200, 200);
    height: 8px;
    border-radius: 4px;
}

QSlider:disabled
{
    background: rgb(240, 240, 240);
    border: 1px solid rgb(200, 200, 200);
}

/* Progress Bar */

QProgressBar
{
    text-align: center;
    color: rgb(60, 60, 60);
    border: 1px solid rgb(200, 200, 200);
    background: rgb(220, 220, 220);
    height: 10px;
    border-radius: 5px;
}

QProgressBar::chunk
{
    background-color: rgb(100, 100, 100);
    width: 1px;
}

QProgressBar:disabled
{
    color: rgb(150, 150, 150);
    background-color: rgb(240, 240, 240);
    border-color: rgb(200, 200, 200);
}

/* Tab */

QTabBar::tab
{
    color: rgb(60, 60, 60);
    border: 1px solid rgb(150, 150, 150);
    background-color: rgb(220, 220, 220);
    padding: 4px;
    margin: 0 2px 0 0;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

QTabBar::tab:selected
{
    background-color: rgb(200, 200, 200);
}

QTabBar::tab:!selected:hover
{
    background-color: rgb(150, 150, 150);
}

QTabBar::tab:top:!selected
{
    margin-top: 3px;
}

QTabBar::tab:bottom:!selected
{
    margin-bottom: 3px;
}

QTabBar::tab:top, QTabBar::tab:bottom
{
    min-width: 8ex;
    margin: 0px 4px 0px 4px;
    padding: 4px;
}

/* Scroll Area */

QScrollArea
{
    border: 1px solid rgb(150, 150, 150);
    background-color: rgb(240, 240, 240);
}

QScrollArea::corner
{
    background: rgb(220, 220, 220);
}

/* Group Box */

QGroupBox
{
    border: 1px solid rgb(150, 150, 150);
    border-radius: 5px;
    margin-top: 10px;
}

QGroupBox::title
{
    subcontrol-origin: margin;
    left: 7px;
    padding: 0px 5px 0px 5px;
}

